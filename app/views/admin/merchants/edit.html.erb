<h1> Edit <%= @merchant.name %></h1>

<% flash.each do |type, msg| %>
  <div>
    <%= msg %>
  </div>
<% end %><br>

<%= form_with url: admin_merchant_path(id: @merchant.id), method: :patch, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, value: @merchant.name %><br>
  <%= f.submit "Submit" %>
<% end %>
